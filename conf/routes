# Routes
GET         /                        controllers.Application.login

GET         /register                controllers.Application.register
POST        /savecontestent          controllers.Registration.saveContestant

GET         /login                   controllers.Application.login

GET         /upload                  controllers.Application.upload

POST        /receiveReceipt          controllers.Receipts.receiveReceipt
POST        /callback/receive        controllers.Callbacks.receive

GET         /support                 controllers.Application.support
POST        /customersupport         controllers.Support.userSupport

GET         /jsRoutes                controllers.JavascriptRoutes.jsRoutes

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file            controllers.Assets.versioned(path="/public", file: Asset)
